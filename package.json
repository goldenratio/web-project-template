{
  "name": "web-project-template",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && rollup -c rollup.config.js --config-production",
    "build:dev": "npm run clean && rollup -c rollup.config.js",
    "build:watch": "npm run clean && rollup -c rollup.config.server.js -w",
    "check-code-style": "prettier src/**/*.ts",
    "fix-code-style": "prettier --write src/**/*.ts"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.ts": [
      "npm run fix-code-style",
      "git add"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-html": "^0.2.4",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "copy-dir": "^1.1.0",
    "lint-staged": "^9.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.4.1",
    "rimraf": "^2.6.3",
    "rollup": "^2.58.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "rxjs": "^6.6.7"
  }
}
